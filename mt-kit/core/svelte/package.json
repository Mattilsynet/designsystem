{
  "name": "@mattilsynet/designsystem-svelte",
  "version": "0.8.7",
  "description": "Design system for Mattilsynet",
  "private": false,
  "homepage": "https://mattilsynet.github.io/designsystem",
  "gitRepository": "https://github.com/Mattilsynet/designsystem",
  "scripts": {
    "dev": "vite",
    "build": "npm run package",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint",
    "format": "prettier --write '{src,stories}/**/*{.ts,.svelte}'",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "keywords": [
    "Design system",
    "Mattilsynet",
    "Svelte",
    "Storybook"
  ],
  "author": "Mattilsynet<matsdrift@mattilsynet.no>",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./ts": {
      "types": "./dist/ts/index.d.ts",
      "import": "./dist/ts/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "engines": {
    "node": ">= 18.18.0",
    "npm": ">= 9.5.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.16.0",
    "@percy/cli": "^1.30.4",
    "@storybook/addon-a11y": "^8.5.3",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-svelte-csf": "^5.0.0-next.14",
    "@storybook/blocks": "^8.5.3",
    "@storybook/manager-api": "^8.5.3",
    "@storybook/preview-api": "^8.5.3",
    "@storybook/svelte": "^8.5.3",
    "@storybook/sveltekit": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@storybook/theming": "^8.5.3",
    "@sveltejs/kit": "^2.12.2",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.6",
    "@testing-library/user-event": "^14.5.2",
    "@tsconfig/svelte": "^5.0.4",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-svelte": "^2.46.1",
    "jsdom": "^25.0.1",
    "msw": "^2.4.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.1",
    "prettier-plugin-svelte": "^3.3.2",
    "publint": "^0.2.7",
    "sass": "^1.81.1",
    "storybook": "^8.5.3",
    "svelte": "^5.19.6",
    "svelte-check": "^4.1.4",
    "svelte-eslint-parser": "^0.43.0",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.7.3",
