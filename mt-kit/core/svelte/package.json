{
  "name": "@mattilsynet/designsystem-svelte",
  "version": "0.7.6",
  "description": "Design system for Mattilsynet",
  "private": false,
  "homepage": "https://mattilsynet.github.io/designsystem",
  "gitRepository": "https://github.com/Mattilsynet/designsystem",
  "scripts": {
    "dev": "vite",
    "build": "npm run package",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest",
    "test:ci": "vitest run",
    "format": "prettier --write '{src,stories}/**/*{.ts,.svelte}'",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "keywords": [
    "Design system",
    "Mattilsynet",
    "Svelte",
    "Storybook",
    "xstate"
  ],
  "author": "Mattilsynet<matsdrift@mattilsynet.no>",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./ts": {
      "types": "./dist/ts/index.d.ts",
      "import": "./dist/ts/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "engines": {
    "node": ">= 18.18.0",
    "npm": ">= 9.5.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@percy/cli": "^1.28.0",
    "@storybook/addon-a11y": "^8.4.6",
    "@storybook/addon-essentials": "^8.4.6",
    "@storybook/addon-interactions": "^8.4.6",
    "@storybook/addon-links": "^8.4.6",
    "@storybook/addon-svelte-csf": "^4.2.0",
    "@storybook/blocks": "^8.4.6",
    "@storybook/manager-api": "^8.4.6",
    "@storybook/preview-api": "^8.4.6",
    "@storybook/svelte": "^8.4.6",
    "@storybook/sveltekit": "^8.4.6",
    "@storybook/test": "^8.4.6",
    "@storybook/theming": "^8.4.6",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/svelte": "^5.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@tsconfig/svelte": "^5.0.2",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@xstate/inspect": "^0.8.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-svelte": "^2.35.1",
    "jsdom": "^25.0.0",
    "msw": "^2.4.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.2.7",
    "sass": "^1.70.0",
    "storybook": "^8.4.6",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-preprocess": "^5.1.4",
    "typescript": "^5.7.2",
    "vite": "^6.0.2",
    "vitest": "^2.1.6"
  },
  "dependencies": {
    "@mattilsynet/designsystem-css": "^0.2.63",
    "@u-elements/u-datalist": "^0.0.6",
    "@xstate/svelte": "^2.0.1",
    "gh-pages": "^5.0.0",
    "ol": "^8.2.0",
    "xstate": "^4.38.3"
  }
}
