{
  "arrowParens": "avoid",
  "singleQuote": true,
  "svelteSortOrder": "options-scripts-markup-styles",
  "trailingComma": "none",
  "semi": false,
  "printWidth": 100,
  "svelteBracketNewLine": false,
  "plugins": ["prettier-plugin-svelte"],
  "overrides": [
    { "files": "*.svelte",
      "options": { "parser": "svelte" }
    }
  ]
}
