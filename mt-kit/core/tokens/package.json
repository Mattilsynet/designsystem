{
  "name": "mt-tokens",
  "version": "0.0.1",
  "description": "Mattilsynet design tokens",
  "main": "build/tokens.css",
  "files": [
    "/build"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node ./config/build.js > /dev/null",
    "watch": "nodemon --watch src/index.js --exec \"yarn build\"",
    "token": "cd config && style-dictionary build",
    "transform": "node node_modules/token-transformer/cli.js src/tokens.json src/output.json core,proveta,dark --expandTypography=true --expandShadow=true --expandComposition=true --expandBorder=true"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "style-dictionary": "^3.7.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "token-transformer": "^0.0.30"
  }
}
