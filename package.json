{
  "name": "@mattilsynet/designsystem",
  "version": "0.7.2",
  "description": "Design system for Mattilsynet",
  "main": "index.js",
  "private": false,
  "homepage": "https://mattilsynet.github.io/designsystem",
  "scripts": {
    "tokens": "a11y-color-tokens --outputDirPath='src/scss' --colorTokensPath='src/scss/utilities/colors.js'",
    "watch:sass": "sass --no-source-map --watch src/scss:./dist",
    "build:sass": "sass --no-source-map src/scss:./dist ",
    "test": "jest --env=jsdom src",
    "test:watch": "npm run test -- --watch",
    "storybook": "start-storybook -p 6006",
    "start": "npm-run-all tokens build:sass --parallel watch:*",
    "build": "npm-run-all tokens build:sass",
    "build-storybook": "build-storybook",
    "format": "prettier --write '{src,stories}/**/*{.ts,.svelte}'",
    "lint": "eslint --fix --quiet --ext .js,.svelte,.ts '{src,stories}/**/*{.ts,.svelte}'",
    "lint-style": "stylelint src/scss/**/*.scss src/scss/**/**/*.scss src/scss/**/**/**/*.scss",
    "lint-style:fix": "npm run lint-style --fix",
    "prepublishOnly": "npm run lint && npm run build",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "percy": "percy storybook http://localhost:6006"
  },
  "keywords": [
    "Design system",
    "Mattilsynet",
    "Svelte",
    "Storybook",
    "xstate"
  ],
  "author": "Mattilsynet<matsdrift@mattilsynet.no>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@percy/cli": "^1.0.0-beta.72",
    "@percy/storybook": "^4.1.0",
    "@storybook/addon-a11y": "^6.4.12",
    "@storybook/addon-actions": "^6.4.12",
    "@storybook/addon-essentials": "^6.4.12",
    "@storybook/addon-links": "^6.4.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-svelte-csf": "^2.0.1",
    "@storybook/addons": "^6.4.12",
    "@storybook/svelte": "^6.4.12",
    "@storybook/theming": "^6.4.12",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.1",
    "@types/jest": "^28.1.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@xstate/inspect": "^0.6.5",
    "a11y-color-tokens": "^0.5.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.3",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "sass": "^1.47.0",
    "storybook-xstate-addon": "^2.2.2",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.1",
    "svelte": "^3.49.0",
    "svelte-jester": "^2.3.2",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.7",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@xstate/svelte": "^2.0.0",
    "gh-pages": "^3.2.3",
    "xstate": "^4.32.0"
  }
}
