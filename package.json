{
  "name": "@mattilsynet/designsystem",
  "version": "0.0.30",
  "description": "Design system for Mattilsynet",
  "main": "index.js",
  "private": false,
  "homepage": "https://mattilsynet.github.io/designsystem",
  "scripts": {
    "tokens": "a11y-color-tokens --outputDirPath='src/scss' --colorTokensPath='src/scss/utilities/colors.js'",
    "watch:sass": "sass --no-source-map --watch src/scss:./dist",
    "build:sass": "sass --no-source-map src/scss:./dist ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -s ./static -p 6006",
    "start": "npm-run-all tokens build:sass --parallel watch:*",
    "build": "npm-run-all tokens build:sass",
    "build-storybook": "build-storybook",
    "format": "prettier --write '{src,stories}/**/*{.ts,.svelte}'",
    "lint": "eslint --fix --quiet --ext .js,.svelte,.ts '{src,stories}/**/*{.ts,.svelte}'",
    "lint-style": "stylelint src/scss/**/*.scss src/scss/**/**/*.scss src/scss/**/**/**/*.scss",
    "lint-style:fix": "npm run lint-style --fix",
    "prepublishOnly": "npm run lint && npm run build",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "percy": "percy storybook http://localhost:6006"
  },
  "keywords": [
    "Design system",
    "Mattilsynet",
    "Svelte",
    "Storybook",
    "xstate"
  ],
  "author": "Mattilsynet<matsdrift@mattilsynet.no>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@percy/cli": "^1.0.0-beta.70",
    "@percy/storybook": "^4.1.0",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/addons": "^6.3.12",
    "@storybook/svelte": "^6.3.12",
    "@storybook/theming": "^6.3.12",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@xstate/inspect": "^0.5.1",
    "a11y-color-tokens": "^0.5.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.43.4",
    "storybook-xstate-addon": "^1.1.8",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "svelte": "^3.44.1",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.9.5",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@xstate/svelte": "^0.2.0",
    "gh-pages": "^3.2.3",
    "xstate": "^4.26.0"
  }
}
